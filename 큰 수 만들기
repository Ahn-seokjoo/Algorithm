
def solution(number, k):

  stack = [number[0]]
  print(stack)
  for num in number[1:]:
    while len(stack) > 0 and stack[-1] < num and k> 0:
      k -= 1
      stack.pop()
    stack.append(num)
    print(stack)
  if k!=0 :
    stack = stack[:-k]
  print(''.join(stack))

      
solution(number,k)

#스택을 이용해서, k 개수만큼 number에서 빼서, 가장 큰 수를 만든다
#스택을 이용할 생각을 못했음...
